{
  "name": "Hot Tab",
  "description": "Quickly access your hot tabs.",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "sidePanel",
    "storage",
    "tabs"
  ],
  "action": {
    "default_title": "Hot Tab",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "side_panel": {
    "default_path": "side-panel.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "commands": {
    "command-01": {
      "description": "Command Slot 01",
      "suggested_key": {
        "default": "Alt+Shift+A"
      }
    },
