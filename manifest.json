{
  "name": "Hot Tab",
  "description": "Quickly access your hot tabs.",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_title": "Hot Tab",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
